{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "websocket",
      "urls": ["ws://**", "wss://**"],
      "cacheConfig": {
        "strategy": "networkOnly",
        "timeout": "0u"
      }
    },
    {
      "name": "index-html",
      "urls": ["/index.html", "/"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "0s",
        "timeout": "0s"
      }
    }
  ]
}
